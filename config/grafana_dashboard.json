{
  "dashboard": {
    "title": "Finlab Resource Monitoring - Production Dashboard",
    "tags": ["finlab", "resource-monitoring", "autonomous-learning", "production"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 2,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(champion_updates_total[1m]) > 0",
          "iconColor": "green",
          "name": "Champion Updates",
          "tagKeys": "iteration",
          "titleFormat": "Champion Updated",
          "textFormat": "New champion at iteration {{iteration}}"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 12, "w": 12, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "Panel 1: Resource Usage",
        "description": "System resource utilization with alert thresholds at 80%",
        "targets": [
          {
            "expr": "system_memory_usage_bytes / system_memory_total_bytes * 100",
            "legendFormat": "Memory Usage (%)",
            "refId": "A"
          },
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "CPU Usage (%)",
            "refId": "B"
          },
          {
            "expr": "system_disk_percent",
            "legendFormat": "Disk Usage (%)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "showPoints": "never",
              "axisPlacement": "auto",
              "axisLabel": "Usage %",
              "drawStyle": "line",
              "lineInterpolation": "linear"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 80, "color": "red"}
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["last", "max", "mean"]
          }
        },
        "transformations": [],
        "alert": {
          "name": "High Resource Usage Alert",
          "message": "System resource usage exceeded 80% threshold",
          "conditions": [
            {
              "evaluator": {"params": [80], "type": "gt"},
              "operator": {"type": "or"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 12, "w": 12, "x": 12, "y": 0},
        "type": "timeseries",
        "title": "Panel 2: Strategy Performance",
        "description": "Success rate and champion Sharpe ratio with staleness tracking",
        "targets": [
          {
            "expr": "rate(strategy_success_total[5m]) / (rate(strategy_success_total[5m]) + rate(strategy_failure_total[5m])) * 100",
            "legendFormat": "Success Rate (%)",
            "refId": "A"
          },
          {
            "expr": "strategy_sharpe_ratio",
            "legendFormat": "Champion Sharpe Ratio",
            "refId": "B"
          },
          {
            "expr": "champion_staleness_iterations",
            "legendFormat": "Iterations Since Champion Update",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "showPoints": "auto",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 20, "color": "yellow"},
                {"value": 15, "color": "red"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success Rate (%)"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": null, "color": "red"},
                      {"value": 20, "color": "yellow"},
                      {"value": 40, "color": "green"}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Champion Sharpe Ratio"},
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "decimals",
                  "value": 4
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Iterations Since Champion Update"},
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": null, "color": "green"},
                      {"value": 15, "color": "yellow"},
                      {"value": 20, "color": "red"}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["last", "min", "max", "mean"]
          }
        },
        "alert": {
          "name": "Low Success Rate Alert",
          "message": "Strategy success rate below 20% threshold",
          "conditions": [
            {
              "evaluator": {"params": [20], "type": "lt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "10m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "2m",
          "handler": 1,
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 12, "w": 12, "x": 0, "y": 12},
        "type": "timeseries",
        "title": "Panel 3: Diversity Metrics",
        "description": "Population diversity and collapse detection with 0.1 threshold",
        "targets": [
          {
            "expr": "population_diversity",
            "legendFormat": "Current Diversity Score",
            "refId": "A"
          },
          {
            "expr": "unique_strategy_count",
            "legendFormat": "Unique Strategies",
            "refId": "B"
          },
          {
            "expr": "diversity_collapse_detected",
            "legendFormat": "Collapse Detected (1=Yes, 0=No)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 1,
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 15,
              "gradientMode": "opacity",
              "showPoints": "auto",
              "axisPlacement": "auto",
              "axisLabel": "Diversity Score",
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.1, "color": "yellow"},
                {"value": 0.3, "color": "green"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Current Diversity Score"},
              "properties": [
                {
                  "id": "custom.fillOpacity",
                  "value": 25
                },
                {
                  "id": "custom.lineWidth",
                  "value": 3
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Unique Strategies"},
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "max",
                  "value": null
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Collapse Detected (1=Yes, 0=No)"},
              "properties": [
                {
                  "id": "custom.drawStyle",
                  "value": "bars"
                },
                {
                  "id": "custom.fillOpacity",
                  "value": 50
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["last", "min", "max", "mean"]
          }
        },
        "alert": {
          "name": "Diversity Collapse Alert",
          "message": "Population diversity collapsed below 0.1 threshold",
          "conditions": [
            {
              "evaluator": {"params": [0.1], "type": "lt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "noDataState": "alerting",
          "notifications": []
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 12, "w": 12, "x": 12, "y": 12},
        "type": "timeseries",
        "title": "Panel 4: Container Statistics",
        "description": "Docker container tracking and orphaned container detection",
        "targets": [
          {
            "expr": "active_containers",
            "legendFormat": "Active Containers",
            "refId": "A"
          },
          {
            "expr": "orphaned_containers",
            "legendFormat": "Orphaned Containers",
            "refId": "B"
          },
          {
            "expr": "sum(container_memory_usage_bytes) / 1024 / 1024 / 1024",
            "legendFormat": "Total Container Memory (GB)",
            "refId": "C"
          },
          {
            "expr": "avg(container_cpu_usage_percent)",
            "legendFormat": "Avg Container CPU (%)",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "showPoints": "auto",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "lineInterpolation": "linear"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Orphaned Containers"},
              "properties": [
                {
                  "id": "custom.fillOpacity",
                  "value": 30
                },
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "orange"
                  }
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": null, "color": "green"},
                      {"value": 1, "color": "yellow"},
                      {"value": 3, "color": "red"}
                    ]
                  }
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Total Container Memory (GB)"},
              "properties": [
                {
                  "id": "unit",
                  "value": "decgbytes"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Avg Container CPU (%)"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "showLegend": true,
            "calcs": ["last", "max", "mean"]
          }
        },
        "alert": {
          "name": "Orphaned Containers Alert",
          "message": "More than 3 orphaned containers detected",
          "conditions": [
            {
              "evaluator": {"params": [3], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"params": [], "type": "last"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 24},
        "type": "stat",
        "title": "Memory Usage",
        "description": "Current system memory utilization",
        "targets": [
          {
            "expr": "system_memory_usage_bytes / system_memory_total_bytes * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 80, "color": "red"}
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 24},
        "type": "stat",
        "title": "Population Diversity",
        "description": "Current diversity score (0.0-1.0)",
        "targets": [
          {
            "expr": "population_diversity",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.1, "color": "yellow"},
                {"value": 0.3, "color": "green"}
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 24},
        "type": "stat",
        "title": "Orphaned Containers",
        "description": "Containers requiring cleanup",
        "targets": [
          {
            "expr": "orphaned_containers",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 3, "color": "red"}
              ]
            },
            "mappings": []
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 30},
        "type": "table",
        "title": "Alert Summary",
        "description": "Overview of all triggered alerts",
        "targets": [
          {
            "expr": "alerts_triggered_total",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "alert_type"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Alert Type"
                },
                {
                  "id": "custom.width",
                  "value": 300
                }
              ]
            },
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {
                  "id": "displayName",
                  "value": "Total Triggered"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Total Triggered",
              "desc": true
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {}
            }
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "style": "dark",
    "uid": "finlab-resource-monitoring-v2",
    "weekStart": ""
  },
  "overwrite": true
}
